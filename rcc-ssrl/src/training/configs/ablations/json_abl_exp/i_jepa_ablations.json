{
  "version": 1,
  "model": "i_jepa",
  "suite_name": "ijepa_histopatology_ablations",
  "base_config": "src/training/configs/ablations/i_jepa/exp_i_jepa_abl_base.yaml",
  "output_dir": "src/training/configs/ablations/i_jepa",
  "exp_prefix": "exp_i_jepa_abl",
  "sbatch_script": "slurm/train_single_node.sbatch",
  "experiments": [
    {
      "id": 3,
      "config_filename": "exp_i_jepa_abl03.yaml",
      "tags": ["predictor", "depth12"],
      "override": {
        "model": {
          "ssl": {
            "i_jepa": { "predictor_depth": 12 }
          }
        }
      }
    },
    {
      "id": 4,
      "config_filename": "exp_i_jepa_abl04.yaml",
      "tags": ["targets", "num_target_masks6"],
      "override": {
        "model": {
          "ssl": {
            "i_jepa": { "num_target_masks": 6 }
          }
        }
      }
    },
    {
      "id": 5,
      "config_filename": "exp_i_jepa_abl05.yaml",
      "tags": ["targets", "num_target_masks2", "fast"],
      "override": {
        "model": {
          "ssl": {
            "i_jepa": { "num_target_masks": 2 }
          }
        },
        "train": { "ssl": { "accumulate_steps": 12 } }
      }
    },
    {
      "id": 6,
      "config_filename": "exp_i_jepa_abl06.yaml",
      "tags": ["predictor", "width512", "heads8"],
      "override": {
        "model": {
          "ssl": {
            "i_jepa": {
              "predictor_embed_dim": 512,
              "predictor_num_heads": 8
            }
          }
        }
      }
    },
    {
      "id": 7,
      "config_filename": "exp_i_jepa_abl07.yaml",
      "tags": ["predictor", "width256", "efficient"],
      "override": {
        "model": {
          "ssl": {
            "i_jepa": {
              "predictor_embed_dim": 256,
              "predictor_num_heads": 4
            }
          }
        }
      }
    },
    {
      "id": 8,
      "config_filename": "exp_i_jepa_abl08.yaml",
      "tags": ["ema", "ema0990_fixed"],
      "override": {
        "model": { "ssl": { "ema_to_one": false, "ema_m": 0.99 } },
        "train": { "ssl": { "ema_m": 0.99 } }
      }
    },
    {
      "id": 9,
      "config_filename": "exp_i_jepa_abl09.yaml",
      "tags": ["ema", "ema0998_to_one"],
      "override": {
        "model": { "ssl": { "ema_to_one": true, "ema_m": 0.998 } },
        "train": { "ssl": { "ema_m": 0.998 } }
      }
    },
    {
      "id": 10,
      "config_filename": "exp_i_jepa_abl10.yaml",
      "tags": ["optim", "lr5e-4", "wd0.1"],
      "override": {
        "train": {
          "optim": { "lr": 0.0005, "weight_decay": 0.1 }
        }
      }
    },
    {
      "id": 11,
      "config_filename": "exp_i_jepa_abl11.yaml",
      "tags": ["paper_style", "no_strong_aug", "no_mixing"],
      "override": {
        "model": {
          "ssl": {
            "aug": { "jitter": 0.0, "blur_prob": 0.0, "gray_prob": 0.0, "solarize_prob": 0.0 }
          }
        },
        "aug": {
          "stain": {
            "normalize": { "enable": false, "method": "vahadane" },
            "jitter": { "enable": false, "mode": "HED", "delta": 0.0 }
          },
          "mixing": {
            "mixup": { "enable": false, "alpha": 0.3 },
            "cutmix": { "enable": false, "beta": 1.0, "p": 0.5 }
          }
        }
      }
    },
    {
      "id": 12,
      "config_filename": "exp_i_jepa_abl12.yaml",
      "tags": ["mask_scales", "target_0.15_0.25", "context_0.75_1.0", "needs_code_patch"],
      "override": {
        "model": {
          "ssl": {
            "i_jepa": {
              "target_scale_range": [0.15, 0.25],
              "context_scale_range": [0.75, 1.0]
            }
          }
        }
      }
    }
  ]
}
